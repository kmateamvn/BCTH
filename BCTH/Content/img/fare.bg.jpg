<script type='text/javascript' src='http://demo.taxigroup.com.vn/wp-content/plugins/contact-form-7/includes/js/scripts.js?ver=4.2'></script>
<script type="text/javascript">
    var findAnswerElements = function (question, handler) {
        var child = question.next();
        while (child.length != 0 && child.prop("tagName").toLowerCase() != 'h3' && child.prop("tagName").toLowerCase() != 'h2') {
            handler(child);
            child = child.next();
        }
    };

    $(document).ready(function () {
        /******************************************* SUPPORT *******************************************/
        var h2s = jQuery('.display-h2-numbers h2');
        for (var i = 0, l = h2s.length; i < l; i++)
            jQuery(h2s[i]).addClass('h2-number').prepend('<span>' + (i + 1) + '</span>');

        var h3 = $("#content-support h3");
        h3.each(function () {
            var question = $(this);
            question.addClass("state-hide").prepend('<span></span>');
            question.children().addClass(" fa fa-plus");
            findAnswerElements(question, function (child) {
                child.hide();
            })
        });

        h3.click(function () {
            var question = $(this);
            var st = question.children();
            if (question.hasClass('state-hide')) {
                question.addClass('state-show').removeClass('state-hide');
                findAnswerElements(question, function (child) {
                    child.show();
                    question.children().removeClass('fa-plus').addClass("fa-minus");
                });
            } else {
                question.addClass('state-hide').removeClass('state-show');
                findAnswerElements(question, function (child) {
                    child.hide();
                    question.children().removeClass('fa-minus').addClass("fa-plus");
                });
            }
        });

        /******************************************* HEADER *******************************************/
        function header() {
            var x = 130;
            $(window).scroll(function () {
                $("#header").stop(true);
                if ($(this).scrollTop() < 130) {
                    $("#header").animate({
                        "top": "0"
                    }, '0.1');
                    x = $(this).scrollTop();
                } else if ($(this).scrollTop() > x) {
                    $("#header").animate({
                        "top": "-130"
                    }, "0.1");
                    x = $(this).scrollTop();
                } else {
                    $("#header").animate({
                        "top": "0"
                    }, '0.1');
                    x = $(this).scrollTop();
                }
            });
        };
        header();
        /******************************************* MENU *******************************************/
        $("#logo-menu").click(function () {
            $("#menu").addClass("open");
            $("#click").addClass("cl-open")
        });
        $("#logo-menu2").click(function (e) {
            $("#menu").removeClass("open")
        });
        $("#click,#menu-hide").click(function (e) {
            $("#menu").removeClass("open");
            $("#click").removeClass("cl-open")
        });
        $("#legal-menu").click(function (e) {
            $(".lg").toggle();
        });
		
        /***********************************************FARE*****************************************/


        var dataMap = {
            eco: {
                point: 80,
                target: -7,
                value: 'eco',
                bgPosition: '0px 0px'
            },
            noi7: {
                point: 240,
                target: 165,
                value: 'noi7',
                bgPosition: '-70px 0px'
            },
            noi4: {
                point: 406,
                target: 333,
                value: 'noi4',
                bgPosition: '0px 0px'
            },
            fly4: {
                point: 580,
                target: 495,
                value: 'fly4',
                bgPosition: '0px 0px'
            },
            fly7: {
                point: 1000,
                target: 650,
                value: 'fly7',
                bgPosition: '-70px 0px'
            },
        };

        var displayFare = function (data) {
            $('#fare #car-type-switch span.display').animate({
                left: data.target
            }).css('background-position', data.bgPosition);
            $('#fare .car-type-switch-text .active').removeClass('active');
            $('#fare .car-show').attr("src", "img/" + data.value + ".png");
            $('#fare .car-type-switch-text span.' + data.value).addClass('active');
            $('#fare .car-type-data.active').removeClass('active').fadeOut(200);
            $('#car-type-' + data.value).addClass('active').fadeIn(200);
        };

        $('#fare #car-type-switch span.display').draggable({
            axis: "x",
            containment: "parent",
            stop: function (evt, ui) {
                var position = ui.position.left;
                for (var name in dataMap) {
                    if (position < dataMap[name].point) {
                        displayFare(dataMap[name]);
                        break;
                    }
                }
            }
        });

        $('#fare .car-type-switch-text span').click(function () {
            var name = $(this).attr('assign');
            displayFare(dataMap[name]);
        });

        $('#fare #car-type-switch .anchor').click(function () {
            var name = $(this).attr('assign');
            displayFare(dataMap[name]);
        });
        $('#car-type-select').change(function () {
            var name = $(this).val();
            displayFare(dataMap[name]);
        }).change();


        var html = $('html');
        if (html.hasClass('touch')) {
            $('.remove-target-blank-on-touch').removeAttr('target');
        }
    });
</script>